@model IEnumerable<TrustTeamVersion4.Models.Domain.Home>
@{
    ViewData["Title"] = "Table";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Table</h2>
<table class="table table-striped table-condensed table-bordered">
	<tr>
		<th>Number</th>
		<th>Organization name</th>
		<th>Organization number</th>
		<th class="text-right">Opened by user</th>
		<th class="text-right">Open date</th>
		<th></th>
	</tr>
	@foreach (var item in Model)
	{
		<tr>
			<td>@item.Number</td>
			<td>@item.InvoiceOrganizationName</td>
			<td>@item.OrganizationNumber</td>
			<td class="text-right">@(item.OpenedByUser)</td>
			<td class="text-right">@(item.SupportCallOpenDate)</td>
			@*<td>
					<a asp-controller="Home" asp-action="Index" asp-route-id="@item.Number">Detail</a> |
					<a asp-controller="Home" asp-action="Index" asp-route-id="@item.Number">Edit</a> |
					<a asp-controller="Home" asp-action="Index" asp-route-id="@item.Number">Delete</a>
				</td>*@
		</tr>
	}
</table>

